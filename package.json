{
  "name": "looker-admin-power-pack",
  "version": "0.1.0",
  "description": "Enhanced administration utilities for Looker",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "webpack-dev-server --hot --disable-host-check --port 8080",
    "start-no-hot": "webpack-dev-server --no-inline --no-hot --port 8080",
    "build": "tsc --noEmit --skipLibCheck && export BABEL_ENV=build && webpack --mode=production --config webpack.prod.config.js",
    "test": "jest"
  },
  "author": "Looker",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@looker/components": "0.9.14",
    "@looker/extension-sdk": "22.4.2",
    "@looker/extension-sdk-react": "22.4.2",
    "@looker/sdk": "^22.4.2",
    "@types/jest": "^26.0.0",
    "@types/papaparse": "^5.0.6",
    "@xstate/react": "^0.8.1",
    "cronstrue": "^1.94.0",
    "lodash": "^4.17.15",
    "mime": "^2.4.6",
    "papaparse": "^5.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.0.4",
    "style-loader": "^1.1.3",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.2",
    "tiny-async-pool": "^1.1.0",
    "xstate": "^4.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.9.2",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-table": "^7.0.14",
    "@types/readable-stream": "^2.3.5",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.3",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-nano-react-app": "^0.1.0",
    "css-loader": "^3.4.2",
    "jest": "^26.0.1",
    "prettier": "2.6.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.6.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
